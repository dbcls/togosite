<script type="module" src="https://togostanza.github.io/metastanza/hash-table.js" async></script>
<script type="module" src="https://togostanza.github.io/metastanza/barchart.js" async></script>

<h2>Ensembl Gene {{id}} summary</h2>

<togostanza-hash-table
  data-url="https://integbio.jp/togosite/sparqlist/api/togosite-gene-summary-ensembl?ensg={{id}}"
  data-type="json"
  width="800"
  height="400"
  padding="0"
  columns="[{&quot;id&quot;: &quot;desc&quot;, &quot;label&quot;: &quot;Gene Name&quot;}, {&quot;id&quot;: &quot;gene_symbol&quot;, &quot;label&quot;: &quot;HGNC Symbol&quot;}, {&quot;id&quot;: &quot;type_label&quot;, &quot;label&quot;: &quot;Gene Type&quot;}, {&quot;id&quot;: &quot;ensg_id&quot;, &quot;label&quot;: &quot;Ensembl Gene ID&quot;},  {&quot;id&quot;: &quot;ncbigene&quot;, &quot;label&quot;: &quot;NCBI Gene ID&quot;, &quot;link&quot;: &quot;ncbigene&quot;}]"
  format-key="true"
></togostanza-hash-table>

<h2>{{id}} expression (GTEx version 8)</h2>

<togostanza-barchart
  chart-type="stacked"
  data-url="https://integbio.jp/togosite/sparqlist/api/gtex_log_tpm?queryIds&#x3D;{{id}}"
  data-type="json"
  category="desc"
  value="logTPM"
  group-by="tissueCategory"
  category-title="Tissue"
  value-title="Expression value (log(TPM+1))"
  legend-title="Tissue"
  width="800"
  height="300"
  padding="50"
  padding-inner="0.3"
  padding-outer="0.4"
  xaxis-placement="bottom"
  yaxis-placement="left"
  xgrid="false"
  ygrid="true"
  xtick="false"
  ytick="true"
  xlabel-angle="90"
  ylabel-angle="0"
  xlabel-padding="50"
  ylabel-padding="5"
  xtitle-padding="10"
  ytitle-padding="10"
  bar-width="0.8"
  legend="true"
  metastanza-menu-placement="top-right"
></togostanza-barchart>
